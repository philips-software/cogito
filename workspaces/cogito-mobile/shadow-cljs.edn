;; shadow-cljs configuration
{:source-paths
 ["src/main"
  "src/test"]

 :dependencies
 [[reagent "0.8.1"]]

 :builds
 {:cogito
  {:target :react-native
   :init-fn cogito.app/init
   :output-dir "build"
   :devtools {:autoload true}}
  :test
  {:target :node-test
   :output-to "build/node-tests.js"
   :ns-regexp "-test$"
   :autorun true}}}
